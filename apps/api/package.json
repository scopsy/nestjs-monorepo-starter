{
  "name": "@nest-starter/api",
  "version": "1.2.5",
  "description": "description",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "node --max-old-space-size=1500 node_modules/.bin/tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "precommit": "lint-staged",
    "start": "TZ=UTC ts-node src/main.ts",
    "start:dev": "cross-env TZ=UTC nodemon",
    "start:debug": "TZ=UTC nodemon --config nodemon-debug.json",
    "start:prod": "TZ=UTC node dist/main.js",
    "lint": "eslint --fix",
    "test": "cross-env TZ=UTC NODE_ENV=test mocha --timeout 10000 --require ts-node/register --exit src/**/**/*.spec.ts",
    "test:e2e": "cross-env TZ=UTC NODE_ENV=test mocha --timeout 10000 --require ts-node/register --exit --file e2e/setup.ts e2e/**/*.e2e.ts"
  },
  "dependencies": {
    "@nestjsx/crud": "^4.6.2",
    "@godaddy/terminus": "^4.3.1",
    "@nest-starter/core": "^1.2.4",
    "@nest-starter/shared": "^1.1.70",
    "@nestjs/common": "^7.4.2",
    "@nestjs/core": "^7.4.2",
    "@nestjs/graphql": "^7.3.4",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/mongoose": "^7.0.2",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.4.2",
    "@nestjs/swagger": "^4.5.12",
    "@nestjs/terminus": "^7.0.1",
    "@octokit/graphql": "^4.3.1",
    "@octokit/rest": "^18.0.3",
    "@sentry/node": "^5.15.5",
    "@slack/events-api": "^2.3.2",
    "@slack/web-api": "^5.8.0",
    "@types/bcrypt": "^3.0.0",
    "@types/fs-extra": "^8.1.0",
    "@types/passport-github": "^1.1.5",
    "@types/puppeteer": "^3.0.1",
    "@types/semver": "^7.1.0",
    "apollo-server-express": "^2.12.0",
    "badgen": "^3.0.1",
    "bcrypt": "^5.0.0",
    "chrono-node": "^2.1.5",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "envalid": "^6.0.1",
    "fs-extra": "^9.0.0",
    "graphql": "^15.0.0",
    "graphql-tools": "^6.0.17",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "nest-raven": "^7.0.0",
    "normalize-url": "^5.0.0",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-google-oauth": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "puppeteer": "^5.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "semver": "^7.3.2",
    "showdown": "^1.9.1",
    "swagger-ui-express": "^4.1.4",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.4.2",
    "@types/chai": "^4.2.11",
    "@types/express": "4.17.7",
    "@types/faker": "^5.1.0",
    "@types/graphql": "^14.5.0",
    "@types/mocha": "^8.0.1",
    "@types/node": "^14.6.0",
    "@types/passport-google-oauth": "^1.0.41",
    "@types/passport-jwt": "^3.0.3",
    "@types/sinon": "^9.0.0",
    "@types/supertest": "^2.0.8",
    "chai": "^4.2.0",
    "faker": "^5.1.0",
    "mocha": "^8.1.1",
    "nodemon": "^2.0.3",
    "sinon": "^9.0.2",
    "superagent": "^6.0.0",
    "superagent-defaults": "^0.1.14",
    "supertest": "^5.0.0",
    "ts-loader": "^8.0.1",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": ["yarn lint", "prettier --ignore-path .eslintignore --write"],
    "{*.json,.{babelrc,eslintrc,prettierrc,stylelintrc}}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ],
    "*.{html,md,yml}": ["prettier --ignore-path .eslintignore --single-quote --write"]
  }
}
